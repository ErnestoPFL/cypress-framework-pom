{
    "extends": ["prettier"],
    "plugins": ["cypress", "prettier"],
    "root": true,
    "env": {
      "cypress/globals": true
    },
    "rules": {
      "import/prefer-default-export": "off",
      "max-len": "off",
      "func-names": "off",
      "prefer-arrow-callback": "off",
      "no-underscore-dangle": "off",
      "no-unused-expressions": "off",
      "arrow-body-style": "off",
      "prettier/prettier": "error",
      "camelcase": "off"
    }
  }
  